{
  "serviceID": "",
  "http": {
    "publicEndpoint": "http://public-endpoint",
    "bindAddr": "0.0.0.0",
    "bindPort": 8085
  },
  "registry": {
    "backend": {
      "type": "leveldb",
      "dsn": "/data/registry"
    },
    "retentionPeriods": ["1h", "1w"]
  },
  "data": {
    "backend": {
      "type": "sqlite",
      "dsn": "/data/data.db"
    },
    "autoRegistration": false
  }
}
