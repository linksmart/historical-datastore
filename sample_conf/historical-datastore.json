{
  "serviceID": "",
  "http": {
    "publicEndpoint": "http://public-endpoint",
    "bindAddr": "0.0.0.0",
    "bindPort": 8085
  },
  "web": {
    "bindAddr": "0.0.0.0",
    "bindPort": 4000,
    "staticDir": "../web"
  },
  "registry": {
    "backend": {
      "type": "leveldb",
      "dsn": "./hds/registry"
    },
    "retentionPeriods": ["1h", "1w"]
  },
  "data": {
    "backend": {
      "type": "senmlstore",
      "dsn": "./hds/data"
    },
    "autoRegistration": false
  },
  "serviceCatalog": {},
  "auth": {}
}
